<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/history_layout"
    tools:context=".History">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background_grey"
        android:orientation="vertical"
        android:weightSum="1.0">

        <LinearLayout
            android:id="@+id/layout_seven"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1428571428571429"
            tools:ignore="Suspicious0dp"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:id="@+id/rel_lay_one_week_ago"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="60"
                tools:ignore="Suspicious0dp"
                android:background="@color/blue">
                <include
                    android:id="@+id/customView7"
                    layout="@layout/custom_layout"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_six"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1428571428571429"
            tools:ignore="Suspicious0dp"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:id="@+id/rel_lay_six_days_ago"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20"
                tools:ignore="Suspicious0dp"
                android:background="@color/green">
                <include
                    android:id="@+id/customView6"
                    layout="@layout/custom_layout"/>

            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/layout_five"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1428571428571429"
            tools:ignore="Suspicious0dp"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:id="@+id/rel_lay_five_days_ago"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="40"
                tools:ignore="Suspicious0dp"
                android:background="@color/yellow">
                <include
                    android:id="@+id/customView5"
                    layout="@layout/custom_layout"/>

            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/layout_four"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1428571428571429"
            tools:ignore="Suspicious0dp"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:id="@+id/rel_lay_four_days_ago"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20"
                tools:ignore="Suspicious0dp"
                android:background="@color/grey">
                <include
                    android:id="@+id/customView4"
                    layout="@layout/custom_layout"/>

            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/layout_three"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1428571428571429"
            tools:ignore="Suspicious0dp"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:id="@+id/rel_lay_three_days_ago"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="100"
                tools:ignore="Suspicious0dp"
                android:background="@color/green">
                <include
                    android:id="@+id/customView3"
                    layout="@layout/custom_layout"/>

            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/layout_two"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1428571428571429"
            tools:ignore="Suspicious0dp"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:id="@+id/rel_lay_day_before_yesterday"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="40"
                tools:ignore="Suspicious0dp"
                android:background="@color/yellow">
                <include
                    android:id="@+id/customView2"
                    layout="@layout/custom_layout"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_one"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.1428571428571429"
            tools:ignore="Suspicious0dp"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:id="@+id/rel_lay_yesterday"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20"
                tools:ignore="Suspicious0dp"
                android:background="@color/red">
                <include
                    android:id="@+id/customView1"
                    layout="@layout/custom_layout"/>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>